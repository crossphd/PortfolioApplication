
<br />
<% @projects.each do |project| %>
<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1 class="allign-left"><%= link_to project.name, project.address, :target => "_blank"  %></h1>
  <br />
  <p><%= project.description %></p>
  <br />
  <div class="pull-right">
  <%= link_to 'Go to App', project.address, class: "btn btn-warning", :target => "_blank" %>
  </div>
</div>
<% end %>





